{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"moduleResolution": "Bundler",
		"noEmit": true,
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"rootDir": "src"
	},
	"exclude": [
		"node_modules"
	]
}
